pipeline{
agent any
  tools{
     maven 'mymaven'
  }

stages{
stage('clone Repo')
  {
    steps{
       git 'https://github.com/fdgvg/DevOpsCodeDemo.git'     
    }
  }
stage('Build Code')
  {
    steps{
       sh 'mvn package'   
    }
  }

stage('Deploy Code')
  {
    steps{
      deploy adapters: [tomcat9(credentialsId: '7d2ac9bb-ab09-4dd2-9bca-4bb376746781', path: '', url: 'localhost:8080/')], contextPath: null, war: '**/*.war'
    }
  }
}

  
}
